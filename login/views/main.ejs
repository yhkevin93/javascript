<table class="table">
   <caption>用户信息</caption>
   <thead>
      <tr>
         <th>名称</th>
         <th>年龄</th>
         <th>身高</th>
         <th>创建时间</th>
      </tr>
   </thead>
   <tbody>
   	<% 用户.forEach(function(user,index){  %>
      <tr>
         <td><%=user.name%></td>
         <td><%=user.age%></td>
         <th><%=user.high%></th>
         <td><%=user.createdAt%></td>
      </tr>
      <% }); %>
   </tbody>
</table>



<form action="/onloadImg" enctype="multipart/form-data" method="post">
    <!--<input type="text" name="title">-->
    <input type="file" name="data" multiple="multiple"><br>
    <input type="submit" value="Upload">
    </form>
    
    
<ul class="modal-footer pagination pull-right">
	<li <% if(分页.当前页 == 1){ %> class="disabled" <% } %>><a href="?page=1" >«</a></li>
	<% for (var 计数器=1;计数器<=分页.总页数;计数器++){ %>
		<li <% if(分页.当前页 == 计数器){ %> class="active" <% } %> ><a href="?page=<%= 计数器 %>"><%= 计数器 %></a></li>
	<% } %>
	<li  <% if(分页.总页数 == 分页.当前页){ %> class="disabled" <% } %>><a href="?page=<%= 分页.总页数 %>">»</a></li>
</ul>